
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Role{
  STAFF
  ADMIN
}

model User{
  id String @id @default(uuid())
  name String
  email String @unique
  passwird String
  role Role @default(STAFF)

  createdAt DateTime @default(now())
  updatedAt DateTime @updateAt
}